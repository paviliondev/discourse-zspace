{{#if canCreateTopic}}
  {{d-button
    id="create-topic"
    class="btn btn-default"
    action=action
    icon="plus"
    disabled=disabled
    label=label}}
  {{d-button
    id="zspace-upload"
    class="btn btn-default"
    href=siteSettings.zspace_upload_wizard_url
    icon="upload"
    label="zspace.upload"}}
{{/if}}